    <dependency>
      <groupId>ejb</groupId>
      <artifactId>ejb</artifactId>
      <version>2.0</version>
      <type>jar</type>
      <url>http://java.sun.com/products/ejb/docs.html</url>
      <properties>
        <idegaweb.bundle.include>true</idegaweb.bundle.include>
      </properties>
    </dependency>
    <dependency>
      <groupId>jta</groupId>
      <artifactId>jta</artifactId>
      <version>spec1_0_1</version>
      <type>jar</type>
      <url>http://java.sun.com/products/jta/jta-1_0_1B-doc</url>
      <properties>
        <idegaweb.bundle.include>true</idegaweb.bundle.include>
      </properties>
    </dependency>
    <dependency>
      <groupId>javamail</groupId>
      <artifactId>javamail</artifactId>
      <version>1.3</version>
      <type>jar</type>
      <url>http://java.sun.com/products/javamail/</url>
      <properties>
        <idegaweb.bundle.include>true</idegaweb.bundle.include>
      </properties>
    </dependency>
    <dependency>
      <groupId>jaf</groupId>
      <artifactId>jaf</artifactId>
      <version>1.0.2</version>
      <type>jar</type>
      <url>http://java.sun.com/products/javabeans/glasgow/jaf.html</url>
      <properties>
        <idegaweb.bundle.include>true</idegaweb.bundle.include>
      </properties>
    </dependency>
    <dependency>
      <!-- cos is short for com.oreilly.servlet -->

      <groupId>servlets.com</groupId>
      <artifactId>cos</artifactId>
      <version>05Nov2002</version>
      <type>jar</type>
      <url>http://www.servlets.com/cos/</url>
      <properties>
        <idegaweb.bundle.include>true</idegaweb.bundle.include>
      </properties>
    </dependency>
    <dependency>
      <groupId>icu4j</groupId>
      <artifactId>icu4j</artifactId>
      <version>3.0L1</version>
      <!-- 3.0L1 is a special version that Laddi built with is_IS and sv_SE special hollidays -->

      <type>jar</type>
      <url>http://oss.software.ibm.com/icu4j/</url>
      <properties>
        <idegaweb.bundle.include>true</idegaweb.bundle.include>
      </properties>
    </dependency>
    <dependency>
      <groupId>myfaces</groupId>
      <artifactId>myfaces-jsf-api</artifactId>
      <version>1.0.7</version>
      <type>jar</type>
      <url>http://java.sun.com/j2ee/javaserverfaces/</url>
      <properties>
        <idegaweb.bundle.include>true</idegaweb.bundle.include>
      </properties>
    </dependency>
    <dependency>
      <groupId>myfaces</groupId>
      <artifactId>myfaces-components</artifactId>
      <version>1.0.7</version>
      <type>jar</type>
      <url>http://www.myfaces.org/</url>
      <properties>
        <idegaweb.bundle.include>true</idegaweb.bundle.include>
      </properties>
    </dependency>
    <dependency>
      <groupId>oracle_jdbc</groupId>
      <artifactId>oracle_jdbc</artifactId>
      <version>10.1.0.2.0</version>
      <type>jar</type>
      <url>http://www.oracle.com/</url>
    </dependency>
    <dependency>
      <groupId>informix_jdbc</groupId>
      <artifactId>informix_jdbc</artifactId>
      <version>9.0</version>
      <type>jar</type>
      <url>http://www.informix.com/</url>
      <properties/>
    </dependency>
    <dependency>
      <groupId>poi</groupId>
      <artifactId>poi</artifactId>
      <version>2.5-final-20040428</version>
      <type>jar</type>
      <url>http://jakarta.apache.org/poi/</url>
      <properties>
        <idegaweb.bundle.include>true</idegaweb.bundle.include>
      </properties>
    </dependency>
    <dependency>
      <groupId>jug</groupId>
      <artifactId>jug</artifactId>
      <version>1.1</version>
      <type>jar</type>
      <url>http://www.doomdark.org/doomdark/proj/jug/</url>
      <properties>
        <idegaweb.bundle.include>true</idegaweb.bundle.include>
      </properties>
    </dependency>
    <!-- jai and jai codec used in bundles com.idega.block.image and is.idega.golf.block.image, not used in core -->

    <dependency>
      <groupId>jai</groupId>
      <artifactId>jai</artifactId>
      <version>1.0</version>
      <type>jar</type>
      <url>http://java.sun.com/products/java-media/jai/</url>
      <properties>
        <idegaweb.bundle.include>true</idegaweb.bundle.include>
      </properties>
    </dependency>
    <!-- see comment above -->

    <dependency>
      <groupId>jai</groupId>
      <artifactId>jai_codec</artifactId>
      <version>1.0</version>
      <type>jar</type>
      <url>http://java.sun.com/products/java-media/jai/</url>
      <properties>
        <idegaweb.bundle.include>true</idegaweb.bundle.include>
      </properties>
    </dependency>
    <!-- iText used in bundles com.idega.block.reports, com.idega.block.contract, com.idega.block.pdf and campus not used in core -->

    <dependency>
      <groupId>itext</groupId>
      <artifactId>itext</artifactId>
      <version>0.99</version>
      <type>jar</type>
      <url>http://itext.sourceforge.net/</url>
      <properties>
        <idegaweb.bundle.include>true</idegaweb.bundle.include>
      </properties>
    </dependency>
    <!-- commons-logging used in com.idega.faces and com.idega.block.creditcard, not used in core -->

    <dependency>
      <groupId>commons-logging</groupId>
      <artifactId>commons-logging</artifactId>
      <version>1.0.4</version>
      <type>jar</type>
      <url>http://commons.apache.org/</url>
      <properties>
        <idegaweb.bundle.include>true</idegaweb.bundle.include>
      </properties>
    </dependency>
    <dependency>
      <groupId>commons-fileupload</groupId>
      <artifactId>commons-fileupload</artifactId>
      <version>1.0</version>
      <type>jar</type>
      <url>http://jakarta.apache.org/commons/</url>
    </dependency>
    <dependency>
      <groupId>javaldap</groupId>
      <artifactId>java-ldapserver</artifactId>
      <version>0.4</version>
      <type>jar</type>
      <url>http://codehaus.org/~trygvis/ldap</url>
      <properties>
        <idegaweb.bundle.include>true</idegaweb.bundle.include>
      </properties>
    </dependency>
  </dependencies>
  <properties/>
