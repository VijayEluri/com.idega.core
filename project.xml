  <dependencies>
    <dependency>
      <groupId>jdom</groupId>
      <artifactId>jdom</artifactId>
      <version>1.0</version>
      <type>jar</type>
      <url>http://www.jdom.org/</url>
      <properties>
        <idegaweb.bundle.include>true</idegaweb.bundle.include>
      </properties>
    </dependency>
    <dependency>
      <groupId>servletapi</groupId>
      <artifactId>servletapi</artifactId>
      <version>2.4</version>
      <type>jar</type>
