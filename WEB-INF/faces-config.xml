<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<faces-config>

	<component>
		<component-type>Page</component-type>
		<component-class>com.idega.presentation.Page</component-class>
	</component>
	
	<component>
		<component-type>HelpButton</component-type>
		<component-class>com.idega.presentation.ui.HelpButton</component-class>
	</component>
	
	<component>
		<component-type>Help</component-type>
		<component-class>com.idega.idegaweb.help.presentation.Help</component-class>
	</component>
	
	<component>
	    <component-type>DownloadLink</component-type>
	    <component-class>com.idega.presentation.text.DownloadLink</component-class>
	    <property>
	      <display-name>Download link download writer class object</display-name>
		      <property-name>downloadWriter</property-name>
		      <property-class>java.lang.Class</property-class>
	    </property>
	</component>
	
	<managed-bean>
		<managed-bean-name>pageSessionPoller</managed-bean-name>
		<managed-bean-class>com.idega.business.PageSessionPoller</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>

</faces-config>
