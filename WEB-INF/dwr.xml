<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN" "http://www.getahead.ltd.uk/dwr//dwr20.dtd">

<dwr>
	<allow>
		<create creator="ibo" javascript="ChooserService" scope="application">
			<param name="class" value="com.idega.business.chooser.ChooserService" />
		</create>
		<create creator="jsf" javascript="PageSessionPoller" scope="application">
	    	<param name="managedBeanName" value="pageSessionPoller"/>
	    	<param name="class" value="com.idega.business.PageSessionPoller"/>
    	</create>
		<convert converter="bean" match="com.idega.builder.bean.AdvancedProperty" />
		<create creator="ibo" javascript="UserBusiness" scope="application">
			<param name="class" value="com.idega.user.business.UserBusiness" />
			<include method="setPreferredRoleAndGetHomePageUri"/>
		</create>
		<create creator="jsf" javascript="GroupsFilterEngine">
			<param name="managedBeanName" value="groupsFilterEngine"></param>
			<param name="class" value="com.idega.group.business.GroupsFilterEngine"></param>
			<include method="getFilteredGroups"/>
		</create>
	</allow>
	<signatures>
		<![CDATA[
			import java.util.List;
			import com.idega.builder.bean.AdvancedProperty;

			ChooserService.setModuleProperty(String, String, List<AdvancedProperty>);
		]]>
	</signatures>
</dwr>